<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.Green">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#297A2C</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">#5FCC63</item>
        <item name="android:colorButtonNormal">#0E4A11</item>
        <item name="android:textColor">#305024</item>
    </style>

    <style name="AppTheme.Dark">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">#7A7A7A</item>
        <item name="android:colorButtonNormal">#191939</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="AppTheme.Blue">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#2B3BF1</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">#5FB6CC</item>
        <item name="android:colorButtonNormal">#0024B5</item>
        <item name="android:textColor">#2F2450</item>
    </style>



</resources>
